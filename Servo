from gpiozero import Servo
from time import sleep

myGPIO=17

myCorrection=0.0
maxPW=(1.75+myCorrection)/1000
minPW=(1.25-myCorrection)/1000

launchServo = Servo(myGPIO,min_pulse_width=minPW,max_pulse_width=maxPW)

launchServo = Servo(myGPIO)

while True:
  launchServo.min()
  sleep(10)
  launchServo.max()
  sleep(5)
print("Perfect launch just like every single other time")
